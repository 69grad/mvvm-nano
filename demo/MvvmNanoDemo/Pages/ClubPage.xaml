<?xml version="1.0" encoding="UTF-8"?>
<forms:MvvmNanoContentPage x:Class="MvvmNanoDemo.Pages.ClubPage"
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:viewModels="clr-namespace:MvvmNanoDemo.ViewModels;assembly=MvvmNanoDemo"
    xmlns:forms="clr-namespace:MvvmNano.Forms;assembly=MvvmNano.Forms" 
    x:TypeArguments="viewModels:ClubViewModel"
    Title="{Binding Name}">
    <forms:MvvmNanoContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Country}" />
        </StackLayout>
    </forms:MvvmNanoContentPage.Content>
</forms:MvvmNanoContentPage>